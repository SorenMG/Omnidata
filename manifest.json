{
    "name": "Omnidata",
    "version": "1.6",
    "manifest_version": 2,

    "description": "Moving links, text and files made easier than ever.",
    
    "author": "Søren Møller Hansen og Daniel Vestergaard Nielsen - Programmeringsprojekt",
    
    "permissions": [
        "http://webhotel.herningsholm.dk/soer9417/*",
        "https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css",
        "https://fonts.googleapis.com/css?family=Raleway"
    ],
    
    "browser_action": {  
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        },
        "default_popup": "start.html",
        "default_title": "Omnidata"
    }
}